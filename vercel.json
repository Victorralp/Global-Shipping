{
  "rewrites": [
    { "source": "/about", "destination": "/about.html" }
  ],
  "redirects": [
    { "source": "/old", "destination": "/new", "permanent": true }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [{ "key": "X-Custom-Header", "value": "MyValue" }]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
